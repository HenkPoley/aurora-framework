{
	"name": "afterlogic/aurora-framework",
	"version": "0.4.0",
	"config": {
		"github-protocols": ["https"]
	},
	"source": {
		"type": "git",
		"url": "https://github.com/vasil-sokolov/aurora-framework",
		"reference": "master"
	},
	"dist": {
		"type": "zip",
